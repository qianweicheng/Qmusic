<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CommonTitle">
        <attr name="common_title" format="string" />
        <attr name="right_text" format="string" />
        <attr name="right_img" format="reference" />
        <attr name="left_img" format="reference" />
        <attr name="left_text" format="string" />
    </declare-styleable>
    <declare-styleable name="SectionTab">

        <!-- =================== -->
        <!-- Tab styles -->
        <!-- =================== -->
        <attr name="selectedTab">
            <enum name="tab0" value="0" />
            <enum name="tab1" value="1" />
            <enum name="tab2" value="2" />
        </attr>
        <attr name="activeType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundImage">
        <attr name="roundradius" format="dimension" />
        <attr name="blackwhite" format="boolean" />
    </declare-styleable>

</resources>